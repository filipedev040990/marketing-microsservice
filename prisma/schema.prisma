// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Lead {
  id          String    @id
  identifier  String
  name        String
  email       String
  document    String
  birthDate   DateTime
  status      String
  phoneNumber String
  createdAt   DateTime
  updatedAt   DateTime?

  @@map("leads")
}
